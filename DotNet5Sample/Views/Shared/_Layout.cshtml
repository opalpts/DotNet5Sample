<body>
    <header>
        @*<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">*@
        <div class="container-fluid">
            <div class="d-flex navbar mt-0 mb-0 pb-0 pt-1">
                <ul class="nav">
                    <img src="image/Logo.png" alt="EXIM" width="150px" height="30px" class="mt-3" />

                    <li class="nav-item pl-5">
                        <a class="nav-link" href="#">Dashboard</a>
                    </li>
                    <li class="nav-item pl-5">
                        <a class="nav-link" href="#">ข้อมูลผลิตภัณฑ์</a>
